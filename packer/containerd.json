{
  "variables": {
    "iso_url": null,
    "iso_checksum": null
  },
  "builders": [
    {
      "type": "arm-image",
      "iso_url": "{{user `iso_url`}}",
      "iso_checksum_type": "sha256",
      "iso_checksum": "{{user `iso_checksum`}}",
      "image_type": "raspberrypi"
    }
  ],
  "provisioners": [
    {
      "type": "shell",
      "inline": [
        "cat /etc/os-release"
      ]
    }
  ],
  "post-processors": [
    {
      "type": "checksum",
      "output": "manifest.json",
      "strip_path": true
    }
  ]
}
